modules = ["nodejs-20", "web", "postgresql-16"]
run = "npm run dev"
hidden = [".config", ".git", "generated-icon.png", "node_modules", "dist"]

[nix]
channel = "stable-25_05"

[deployment]
deploymentTarget = "autoscale"
build = ["npm", "run", "build"]
run = ["npm", "run", "start"]

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 37405
externalPort = 3000

[[ports]]
localPort = 38119
externalPort = 4200

[[ports]]
localPort = 39949
externalPort = 6800

[[ports]]
localPort = 40133
externalPort = 3002

[[ports]]
localPort = 41305
externalPort = 6000

[[ports]]
localPort = 42219
externalPort = 5000

[[ports]]
localPort = 43075
externalPort = 3001

[[ports]]
localPort = 44109
externalPort = 5173

[[ports]]
localPort = 44489
externalPort = 3003

[env]
PORT = "5000"

[agent]
integrations = ["javascript_object_storage:1.0.0", "javascript_websocket:1.0.0", "firebase_barebones_javascript:1.0.0", "javascript_stripe:1.0.0", "javascript_log_in_with_replit:1.0.0", "javascript_database:1.0.0", "javascript_openai:1.0.0"]
expertMode = true

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Start application"

[[workflows.workflow]]
name = "Start application"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run dev"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"
